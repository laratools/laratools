{
    "name": "laratools/laratools",
    "description": "A collection of useful everyday tools for Laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Wade Urry",
            "email": "wade@iwader.co.uk",
            "homepage": "https://iwader.co.uk/",
            "role": "Developer"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "~7.2",
        "illuminate/contracts": "^6.0|^7.0",
        "illuminate/database": "^6.0|^7.0",
        "illuminate/events": "^6.0|^7.0",
        "illuminate/http": "^6.0|^7.0",
        "ramsey/uuid": "^3.6"
    },
    "require-dev": {
        "illuminate/session": "^6.0|^7.0",
        "mockery/mockery": "^0.9.7",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Laratools\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laratools\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laratools\\Providers\\LaratoolsServiceProvider"
            ]
        }
    }
}
